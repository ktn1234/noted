{
  "name": "noted",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && sed -i 's/__URL_PLACEHOLDER__/https:\\/\\/noted-eight-bay.vercel.app/g' dist/index.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gen:pwa": "npx pwa-asset-generator ./public/sticky-notes.png assets --background \"#04364A\" --xhtml -w -t png -m && mv assets public/assets",
    "gen:supabase": "npx supabase gen types typescript --project-id \"$PROJECT_REF\" --schema public > src/lib/supabase/database.types.ts",
    "docker:pull:cloudbeaver": "docker pull dbeaver/cloudbeaver",
    "docker:run:cloudbeaver:foreground": "docker run --name cloudbeaver --rm -ti -p 8080:8978 -v $(pwd)/workspace dbeaver/cloudbeaver:latest",
    "docker:run:cloudbeaver:background": "docker run --name cloudbeaver -ti -p 8080:8978 -v $(pwd)/workspace -d --restart unless-stopped dbeaver/cloudbeaver:latest",
    "docker:stop:cloudbeaver": "docker container stop cloudbeaver",
    "docker:remove:cloudbeaver": "docker container rm cloudbeaver",
    "docker:clean:images": "docker rmi $(docker images --filter 'dangling=true' -q --no-trunc) || echo 'No dangling images, cleanup complete.'",
    "docker:clean:volumes": "docker volume rm $(docker volume ls -qf dangling=true) || echo 'No dangling volumes, cleanup complete.'",
    "docker:logs": "docker logs -f"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "pwa-asset-generator": "^6.3.1",
    "supabase": "^1.167.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.0",
    "workbox-build": "^7.1.0",
    "workbox-window": "^7.1.0"
  }
}
