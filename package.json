{
  "name": "noted",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && sed -i 's/__URL_PLACEHOLDER__/https:\\/\\/noted-eight-bay.vercel.app/g' dist/index.html && node src/scripts/add-push-notification-event-listener.js",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\" \"supabase/migrations/**/*.sql\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\" \"supabase/migrations/**/*.sql\"",
    "preview": "vite preview --host --port 5173",
    "gen:pwa": "npx pwa-asset-generator ./public/sticky-notes.png assets --background \"#04364A\" --xhtml -w -t png -m && mv assets public/assets",
    "gen:supabase": "npx supabase gen types typescript --project-id \"$PROJECT_REF\" --schema public > src/lib/supabase/database.types.ts",
    "gen:vapid-keys": "npx web-push generate-vapid-keys --json",
    "docker:pull:cloudbeaver": "docker pull dbeaver/cloudbeaver",
    "docker:run:cloudbeaver:foreground": "docker run --name cloudbeaver --rm -ti -p 8080:8978 -v $(pwd)/workspace dbeaver/cloudbeaver:latest",
    "docker:run:cloudbeaver:background": "docker run --name cloudbeaver -ti -p 8080:8978 -v $(pwd)/workspace -d --restart unless-stopped dbeaver/cloudbeaver:latest",
    "docker:stop:cloudbeaver": "docker container stop cloudbeaver",
    "docker:remove:cloudbeaver": "docker container rm cloudbeaver",
    "docker:clean:images": "docker rmi $(docker images --filter 'dangling=true' -q --no-trunc) || echo 'No dangling images, cleanup complete.'",
    "docker:clean:volumes": "docker volume rm $(docker volume ls -qf dangling=true) || echo 'No dangling volumes, cleanup complete.'",
    "docker:logs": "docker logs -f",
    "supabase:fn:push:deploy": "supabase functions deploy note-created-push-notification --debug",
    "supabase:fn:push:delete": "supabase functions delete note-created-push-notification",
    "supabase:secrets:set": "supabase secrets set --env-file ./supabase/.env",
    "supabase:secrets:unset": "supabase secrets unset",
    "supabase:secrets:list": "supabase secrets list",
    "supabase:login": "supabase login",
    "supabase:init": "supabase init",
    "supabase:link": "supabase link --project-ref qvtivjvkltrrwxbwwzwy",
    "supabase:gen:types": "supabase gen types typescript --project-id qvtivjvkltrrwxbwwzwy --schema public > src/lib/supabase/database.types.ts",
    "supabase:db:pull": "supabase db pull --linked",
    "supabase:db:start": "supabase db start",
    "supabase:db:role": "docker exec -it supabase_db_memelord.com psql -U postgres -d postgres -c \"CREATE ROLE algolia_supabase_connector_postgres_1760455475;\"",
    "supabase:db:reset": "supabase db reset",
    "supabase:db:push": "supabase db push --local",
    "supabase:start": "supabase start",
    "supabase:diff": "supabase db diff",
    "supabase:stop": "supabase stop"
  },
  "dependencies": {
    "@emoji-mart/data": "1.2.1",
    "@emoji-mart/react": "1.1.1",
    "@supabase/supabase-js": "2.95.3",
    "@tanstack/react-query": "5.59.15",
    "@tanstack/react-query-devtools": "5.59.15",
    "emoji-mart": "5.6.0",
    "linkify-react": "4.1.3",
    "linkifyjs": "4.1.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-router-dom": "6.27.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@types/web-push": "3.6.3",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.56.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "minify": "11.4.1",
    "postcss": "8.4.47",
    "prettier": "3.8.1",
    "prettier-plugin-sql": "0.19.2",
    "pwa-asset-generator": "6.3.2",
    "supabase": "2.76.3",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vite": "5.4.9",
    "vite-plugin-pwa": "0.20.5",
    "web-push": "3.6.7",
    "workbox-build": "7.1.1",
    "workbox-window": "7.1.0"
  }
}
